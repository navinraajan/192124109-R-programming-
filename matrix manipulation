> matrix1 <- matrix(1:6, nrow = 2, ncol = 3)
> matrix2 <- matrix(7:12, nrow = 2, ncol = 3)
> 
> # Addition
> addition_result <- matrix1 + matrix2
> print(addition_result)

#output:
     [,1] [,2] [,3]
[1,]    8   12   16
[2,]   10   14   18
> 


> # Subtraction
> subtraction_result <- matrix1 - matrix2
> print(subtraction_result)
     [,1] [,2] [,3]

#output
[1,]   -6   -6   -6
[2,]   -6   -6   -6
> 


> # Multiplication (element-wise)
> elementwise_mult_result <- matrix1 * matrix2
> print(elementwise_mult_result)

#output
     [,1] [,2] [,3]
[1,]    7   27   55
[2,]   16   40   72
> 
> # Matrix multiplication
> matrix_mult_result <- matrix1 %*% t(matrix2)  # t() transposes matrix2
> print(matrix_mult_result)
     [,1] [,2]
[1,]   89   98
[2,]  116  128
> 
