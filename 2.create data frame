df <- data.frame(
  name = c("Alice", "Bob", "Charlie"),
  age = c(25, 30, 35),
  city = c("New York", "London", "Paris"),
  stringsAsFactors = FALSE  # Optional: Prevents character columns from being converted to factors
  
)
print(df)

#output
name age     city
1   Alice  25 New York
2     Bob  30   London
3 Charlie  35    Paris




